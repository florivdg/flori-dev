---
import Basic from './Basic.astro'
import Header from '../components/Header.astro'
import Footer from '../components/Footer.astro'
import type { SEOData } from '../types/SEO'

export interface Props {
  seo: SEOData
}

/// SEO
const { seo } = Astro.props
---

<Basic seo={seo}>
  <Header />

  <slot />

  <Footer />
</Basic>
